{
    "builds": [
        {
            "src": "cron/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/cron/(.*)",
            "dest": "cron/index.py"
        }
    ],
    "crons": [
        {
            "path": "/cron",
            "schedule": "0 03 * * *"
        },
        {
            "path": "/cron",
            "schedule": "0 14 * * *"
        }
    ]
}